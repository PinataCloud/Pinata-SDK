{
  "name": "@pinata/sdk",
  "version": "1.1.26",
  "description": "Official SDK for the Pinata IPFS platform",
  "main": "lib/pinata-sdk.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test && tsc",
    "coverage": "jest --coverage",
    "dev": "webpack --progress --colors --watch --env dev",
    "html": "open coverage/index.html",
    "lint": "eslint src --quiet ",
    "lint:fix": "eslint src --fix",
    "test": "./node_modules/.bin/jest --env=node",
    "repl": "node -i -e \"$(< ./lib/pinata-sdk.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PinataCloud/Pinata-SDK.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Matt Ober",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PinataCloud/Pinata-SDK/issues"
  },
  "homepage": "https://github.com/PinataCloud/Pinata-SDK",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.8.3",
    "@types/node": "^15.6.1",
    "acorn": "^7.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "jest": "^25.1.0",
    "typescript": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "base-path-converter": "^1.0.2",
    "form-data": "^2.3.3",
    "is-ipfs": "^0.6.0",
    "recursive-fs": "^1.1.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  }
}
